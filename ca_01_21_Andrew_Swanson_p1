//This program makes the screen 400 x 300, makes the background my favorite color, and prints a square, circle, and rectangle.
void setup()
{
  size(400, 300);
  background(250,250,00);
}
void draw()
{
  fill(250,0,250);
  rect(50,50,50,50);
  fill(0,250,250);
  ellipse(225,75,75,75);
  fill(250,125,125);
  rect(100,150,150,100);
}
